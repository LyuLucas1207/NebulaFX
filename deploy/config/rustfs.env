# NebulaFX administrator username
NEUBULAFX_ROOT_USER=nebulafxadmin
# NebulaFX administrator password
NEUBULAFX_ROOT_PASSWORD=nebulafxadmin

# Data volume configuration example path: deploy/data/nebulafx.env
# NebulaFX data volume storage paths, supports multiple volumes from vol1 to vol4
NEUBULAFX_VOLUMES="./deploy/deploy/vol{1...4}"
# NebulaFX service startup parameters, specifying listen address and port
NEUBULAFX_OPTS="--address :9000"
# NebulaFX service listen address and port
NEUBULAFX_ADDRESS=":9000"
# Enable NebulaFX console functionality
NEUBULAFX_CONSOLE_ENABLE=true
# NebulaFX service domain configuration
NEUBULAFX_SERVER_DOMAINS=127.0.0.1:9000
# NebulaFX license content
NEUBULAFX_LICENSE="license content"
# Observability configuration endpoint: NEUBULAFX_OBS_ENDPOINT
NEUBULAFX_OBS_ENDPOINT=http://localhost:4317
# TLS certificates directory path: deploy/certs
NEUBULAFX_TLS_PATH=/etc/default/tls